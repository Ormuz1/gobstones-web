{
  "env": {
    "browser": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "_": "readonly",
    "$": "readonly",
    "Polymer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 10
  },
  "plugins": [
    "html"
  ],
  "rules": {
    "no-undef": "warn",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  }
}
